{
  "New Routes File": {
    "scope": "javascript,typescript",
    "prefix": "zamdaroute",
    "body": [
      "import { Router } from 'express';",
      "\n",
      "// $1 routers",
      "const $1V1Router = Router();",
      "const $1V2Router = Router();",
      "\n",
      "export {$1V2Router, $1V2Router };",
      "$0"
    ],
    "description": "For routes"
  },
  "OpenApi GET Documentation": {
    "scope": "javascript,typescript",
    "prefix": "getdocs",
    "body": [
      "/**",
      "* @openapi",
      "* $1:",
      "*   get:",
      "*     tags: [$2]",
      "*     summary: GET",
      "*     description: GET",
      "*",
      "*     parameters:",
      "*       - in: query",
      "*         name: page",
      "*         example: 1",
      "*",
      "*     responses:",
      "*       200:",
      "*         content:",
      "*           application/json:",
      "*             example: {}",
      "*/",
      "$0"
    ],
    "description": "For get requests"
  },
  "OpenApi POST 201 Documentation": {
